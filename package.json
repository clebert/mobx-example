{
  "name": "mobx-example",
  "version": "0.0.0",
  "description": "A MobX example project with TypeScript, React and Webpack.",
  "private": true,
  "author": "Clemens Akens",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/mobx.git"
