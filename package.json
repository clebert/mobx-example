    "@types/node": "4.0.22-alpha",
    "@types/react": "0.14.21-alpha",
    "@types/react-dom": "0.14.8-alpha",
    "awesome-typescript-loader": "1.0.0",
    "clean-webpack-plugin": "0.1.9",
    "file-loader": "0.8.5",
    "mobx-react-devtools": "4.2.0",
    "npm-check-updates": "2.6.7",
    "tslint": "3.11.0",
    "tslint-loader": "2.1.4",
    "tslint-microsoft-contrib": "2.0.7",
    "typescript": "next",
    "webpack": "beta",
    "webpack-dev-server": "beta"
